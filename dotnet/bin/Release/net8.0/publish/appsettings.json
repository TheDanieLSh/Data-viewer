{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "https://0.0.0.0:4090",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/etc/ssl/nginx/wildcard.art3d.ru.crt",
        "KeyPath": "/etc/ssl/nginx/wildcard.art3d.ru.pem"
      }
    }
  }
}
